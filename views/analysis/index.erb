<table class="">
  <thead>
    <tr>
      <th>Category</th>
      <th>Merchant</th>
      <th>Transaction</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
    <% @analysis.transactions.each do |transaction| %>
    <tr>
        <td><a href="/transactions/<%= transaction.category.id %>"><%= transaction.category.name %></a></td>
        <td><a href="/transactions/<%= transaction.merchant.id %>"><%= transaction.merchant.name %></a></td>
        <td><a href="/transactions/<%= transaction.id %>"><%= transaction.description %></a></td>
        <td><a href="/transactions/<%= transaction.id %>"><%= transaction.amount %></a></td>
    </tr>    
    <% end %>
    <tr>
      <td></td>
      <td></td>
      <td class="total_label">Total:</td>
      <td class="total_display"><%= @analysis.total %></td>
    </tr>
  </tbody>
</table>